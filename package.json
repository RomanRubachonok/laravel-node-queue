{
  "name": "laravel-queue",
  "version": "1.1.1",
  "description": "run job from laravel",
  "main": "index.js",
  "homepage": "https://github.com/icfr/laravel-node-queue",
  "bugs": "https://github.com/icfr/laravel-node-queue/issues",
  "scripts": {
    "pretest": "./test/scripts/clean.sh",
    "test": "istanbul cover nodeunit -- test/ --reporter tap",
    "jest": "jest",
    "posttest": "./test/scripts/clean.sh",
    "postinstall": "./scripts/install.sh",
    "gen-docs": "jsdoc --configure .jsdoc.json --verbose"
  },
  "bin": {
    "artisan": "./bin/artisan",
    "repl": "./bin/repl"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/icfr/laravel-node-queue.git"
  },
  "keywords": [
    "laravel",
    "job",
    "queue",
    "Broadcast"
  ],
  "jest": {
    "coverageReporters": [
      "json",
      "lcov",
      "text-summary"
    ],
    "testEnvironment": "node",
    "verbose": true,
    "expand": true,
    "useStderr": true,
    "setupFiles": [
      "<rootDir>/test/utils/bootstrap.js"
    ],
    "collectCoverageFrom": [
      "**/*.{ts,js}"
    ],
    "globals": {
      "app": {}
    },
    "coverageDirectory": "/var/www/reports/laravel-queue/jest/",
    "coveragePathIgnorePatterns": [
      "data",
      "test",
      "bin"
    ],
    "collectCoverage": true
  },
  "author": "ICFR",
  "license": "MIT",
  "dependencies": {
    "bee-queue": "^0.3.0",
    "bluebird": "^3.4.6",
    "colors": "^1.1.2",
    "debug-logger": "^0.4.1",
    "diff": "^3.0.0",
    "dotenv": "^2.0.0",
    "each": "^0.6.1",
    "ect": "^0.5.9",
    "fs-promise": "^0.5.0",
    "i18n": "^0.8.3",
    "include-all": "^1.0.6",
    "intercept-stdout": "^0.1.2",
    "ioredis": "^2.4.0",
    "js-beautify": "^1.6.4",
    "lodash": "^4.15.0",
    "lodash-inflection": "^1.3.0",
    "merge": "^1.2.0",
    "mock-stdin": "^0.3.0",
    "mysql": "^2.11.1",
    "nodemailer": "^2.6.4",
    "pad": "^1.0.1",
    "raven": "^0.12.1",
    "readdir": "0.0.13",
    "rewire": "^2.5.2",
    "sequelize": "^3.24.3",
    "shelljs": "^0.7.4",
    "tmp": "0.0.29"
  },
  "devDependencies": {
    "jest": "~22.3.0",
    "minami": "^1.1.1"
  }
}
